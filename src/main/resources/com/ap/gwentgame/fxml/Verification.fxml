<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ap.gwentgame.client.controller.VerificationController">
   <children>
      <VBox alignment="TOP_CENTER" layoutX="125.0" layoutY="25.0" prefHeight="150.0" prefWidth="150.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" maxWidth="150.0" minWidth="150.0" prefHeight="20.0" prefWidth="150.0" text="Please Enter The Code" textFill="#4a3afc" />
            <Label alignment="CENTER" contentDisplay="CENTER" prefWidth="150.0" text="Sent To Your Email" textFill="#4a3afc" />
            <TextField fx:id="sentCode" alignment="CENTER" promptText="sent code">
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin>
               <font>
                  <Font size="13.0" />
               </font>
            </TextField>
            <Button alignment="CENTER" contentDisplay="CENTER" text="ok" onMouseClicked="#checkCode">
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin>
            </Button>
            <Button text="Back" onMouseClicked="#backToRegisterMenu">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Button>
         </children></VBox>
   </children>
</AnchorPane>
