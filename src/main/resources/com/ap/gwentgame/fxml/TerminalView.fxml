<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.ap.gwentgame.client.controller.TerminalController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <Label text="Terminal" style="-fx-font-size: 16px; -fx-font-weight: bold;" />
        <TextArea fx:id="terminalArea" prefHeight="400.0" prefWidth="600.0"
                  style="-fx-control-inner-background: black; -fx-text-fill: white;"/>
    </children>
</VBox>
